{
  "name": "crisiscleanup-3-awsconnect",
  "version": "1.0.0",
  "description": "Crisis Cleanup AWS Connect Lambda Integrations",
  "main": "src/index.js",
  "repository": "https://github.com/CrisisCleanup/crisiscleanup-3-awsconnect",
  "author": "BradenM",
  "license": "MIT",
  "private": false,
  "scripts": {
    "format": "prettier --write ./{*,**/*}.js **/**/*.js ",
    "logs": "sls logs --env dev --stage dev -f",
    "logs:staging": "sls logs --env staging --stage staging -f",
    "offline": "sls --env local offline",
    "update": "node internal/scripts/deploy-functions.js",
    "deploy:dev": "sls deploy --env dev --stage dev --aws-s3-accelerate",
    "deploy:staging": "sls deploy --env staging --stage staging --aws-s3-accelerate",
    "deploy:prod": "sls deploy --env prod --stage prod --aws-s3-accelerate",
    "lint": "eslint --fix ./{*,**/*}.js && yarn format",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@sentry/webpack-plugin": "^1.11.1",
    "axios-mock-adapter": "^1.18.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.3.0",
    "jest-date-mock": "^1.0.8",
    "prettier": "^2.0.2",
    "serverless": "^1.67.3",
    "serverless-domain-manager": "^3.3.1",
    "serverless-dotenv-plugin": "^2.3.2",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.1.4",
    "serverless-plugin-warmup": "^4.9.0",
    "serverless-prune-plugin": "^1.4.2",
    "serverless-sentry": "^1.2.0",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.3.1",
    "shelljs": "^0.8.3",
    "webpack": "^4.42.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@sentry/node": "^5.15.5",
    "aws-sdk": "^2.656.0",
    "axios": "^0.19.2",
    "babel-core": "^7.0.0-bridge.0",
    "raven": "^2.6.4",
    "serverless-sentry-lib": "^1.1.2"
  }
}
